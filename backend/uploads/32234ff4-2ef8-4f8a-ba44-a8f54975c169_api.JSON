                                "body": {
                                    "type": "object",
                                    "properties": {
                                        "total": {
                                            "type": "object",
                                            "description": "总行数"
                                        },
                                        "page": {
                                            "type": "object",
                                            "description": "页码"
                                        },
                                        "size": {
                                            "type": "object",
                                            "description": "每页行数"
